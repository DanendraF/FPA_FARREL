<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml_helloworld.FXMLDocumentController">
    <children>
        <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="69.0" layoutY="138.0" minHeight="16" minWidth="69" prefHeight="18.0" prefWidth="181.0" />
      <RadioButton fx:id="optionA" layoutX="34.0" layoutY="55.0" mnemonicParsing="false" onAction="#handleRadioBAction" text="Jorge M.">
         <toggleGroup>
            <ToggleGroup fx:id="choice" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="optionB" layoutX="34.0" layoutY="85.0" mnemonicParsing="false" onAction="#handleRadioBAction" selected="true" text="Pecco B." toggleGroup="$choice" />
      <RadioButton fx:id="optionC" layoutX="34.0" layoutY="115.0" mnemonicParsing="false" onAction="#handleRadioBAction" text="Fabio Q." toggleGroup="$choice" />
      <Label fx:id="question" layoutX="34.0" layoutY="24.0" text="Siapa rider yang anda suka dibawah ini . . ." />
    </children>
</AnchorPane>
